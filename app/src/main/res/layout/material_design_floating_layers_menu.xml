<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_vazio"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:height="66dp" />

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/action_layer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/action_gps"
        android:src="@drawable/ic_layers_white_24dp"
        fab:fab_label="@string/action_layer"
        fab:fab_size="normal" />

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/action_gps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/tv_vazio"
        android:src="@drawable/ic_gps_not_fixed_white_24dp"
        fab:fab_label="@string/action_gps"
        fab:fab_size="normal" />

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/action_share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/action_layer"
        android:src="@drawable/ic_share_white_24dp"
        fab:fab_label="@string/action_layer"
        fab:fab_size="normal" />

    <!--<com.github.clans.fab.FloatingActionButton
        android:id="@+id/action_rotacao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/action_layer"
        android:src="@drawable/ic_rotation_blocked_white_24dp"
        fab:fab_label="@string/action_rotacao"
        fab:fab_size="normal" />-->
</RelativeLayout>